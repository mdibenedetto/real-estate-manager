{
  "name": "real-estate-manager",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start-ghp": "ng serve --open --configuration=ghp",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "webpack --config webpack.config-prod.js",
    "build-dev": "webpack --config webpack.config-dev.js",
    "start-2": "webpack-dev-server --port=4200 --config webpack.config-dev.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "gh-install": "npm i -g gh-pages",
    "gh-build": "ng build --output-path=dist/gh-deploy --configuration=ghp --prod --base-href \"https://michele2014.github.io/real-estate-manager/\"",
    "gh-deploy": "gh-pages -d dist/gh-deploy",
    "gh-b-d": "npm run gh-build && npm run gh-deploy",
    "util-ncu-install-tool": "npm i -g npm-check-updates",
    "util-ncu-install-latest": "ncu -u && npm install",
    "util-package-lock-false": "npm config set package-lock false",
    "util-fresh-install": "rm -rf node_modules && rm -rf package-lock.json && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.1",
    "@angular/common": "~8.2.1",
    "@angular/compiler": "~8.2.1",
    "@angular/core": "^8.2.1",
    "@angular/forms": "~8.2.1",
    "@angular/platform-browser": "~8.2.1",
    "@angular/platform-browser-dynamic": "~8.2.1",
    "@angular/router": "~8.2.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "jquery": "^3.3.1",
    "popper": "^1.0.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.1",
    "@angular/cli": "^8.2.1",
    "@angular/compiler-cli": "~8.2.1",
    "@angular/language-service": "~8.2.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "gh-pages": "^2.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
