{
    "name": "rem",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "start-ghp": "ng serve --open --configuration=ghp",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-prod": "webpack --config webpack.config-prod.js",
        "build-dev": "webpack --config webpack.config-dev.js",
        "start-2": "webpack-dev-server --port=4200 --config webpack.config-dev.js",
        "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
        "gh-install": "npm i -g gh-pages",
        "gh-build": "ng build --output-path=dist/gh-deploy --env=ghp --prod --base-href \"https://michele2014.github.io/real-estate-manager/\"",
        "gh-deploy": "gh-pages -d dist/gh-deploy",
        "gh-b-d": "npm run gh-build && npm run gh-deploy",
        "util-ncu-install-tool": "npm i -g npm-check-updates",
        "util-ncu-install-latest": "ncu -u && npm install",
        "util-package-lock-false": "npm config set package-lock false",
        "util-fresh-install": "rm -rf node_modules && rm -rf package-lock.json && npm install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "bootstrap": "^4.3.1",
        "core-js": "^2.5.4",
        "jquery": "^3.3.1",
        "popper": "^1.0.1",
        "rxjs": "~6.3.3",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@agm/core": "^1.0.0-beta.5",
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.3.7",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "angular-in-memory-web-api": "github:angular/in-memory-web-api",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2"
    }
}
