{
    "name": "real-estate-manager",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "lint": "ng lint",
        "build": "webpack",
        "build-prod": "webpack --config webpack.config-prod.js",
        "build-dev": "webpack --config webpack.config-dev.js",
        "start": "webpack-dev-server --port=4200 --config webpack.config-dev.js",
        "test": "karma start ./karma.conf.js",
        "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
        "e2e": "protractor ./protractor.conf.js",
        "gh-install": "npm i -g gh-pages",
        "gh-build": "ng build --env=ghp --prod --base-href \"https://michele2014.github.io/real-estate-manager/\"",
        "gh-deploy": "gh-pages -d dist/deploy",
        "util-ncu-install-tool": "npm i -g npm-check-updates",
        "util-ncu-install-latest": "ncu -u && npm install",
        "util-package-lock-false": "npm config set package-lock false",
        "util-fresh-install": "rm -rf node_modules && rm -rf package-lock.json && npm install"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "^1.0.0-beta.2",
        "@angular/animations": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "bootstrap": "^4.0.0",
        "core-js": "^2.4.1",
        "g": "^2.0.1",
        "postcss": "^6.0.19",
        "postcss-merge-rules": "^2.1.2",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular-devkit/core": "^0.3.2",
        "@angular/cli": "^1.7.2",
        "@angular/compiler-cli": "^5.2.6",
        "@angular/language-service": "^5.2.6",
        "@ngtools/webpack": "^1.10.1",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "^6.0.101",
        "autoprefixer": "^7.2.3",
        "circular-dependency-plugin": "^4.2.1",
        "codelyzer": "^4.0.1",
        "copy-webpack-plugin": "^4.1.1",
        "css-loader": "^0.28.1",
        "cssnano": "^3.10.0",
        "exports-loader": "^0.6.3",
        "file-loader": "^1.1.5",
        "gh-pages": "^1.1.0",
        "html-webpack-plugin": "^2.29.0",
        "istanbul-instrumenter-loader": "^3.0.0",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "less-loader": "^4.0.5",
        "postcss-import": "^11.0.0",
        "postcss-loader": "^2.0.10",
        "postcss-url": "^7.1.2",
        "protractor": "~5.1.2",
        "raw-loader": "^0.5.1",
        "sass-loader": "^6.0.6",
        "source-map-loader": "^0.2.0",
        "style-loader": "^0.13.1",
        "stylus-loader": "^3.0.1",
        "ts-node": "~4.1.0",
