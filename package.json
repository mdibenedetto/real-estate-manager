{
  "name": "real-estate-manager",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "build": "webpack",
    "build-prod": "webpack --config webpack.config-prod.js",
    "build-dev": "webpack --config webpack.config-dev.js",
    "start": "webpack-dev-server --port=4200 --config webpack.config-dev.js",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "gh-install": "npm i -g gh-pages",
    "gh-build": "ng build --output-path=dist/gh-deploy --env=ghp --prod --base-href \"https://michele2014.github.io/real-estate-manager/\"",
    "gh-deploy": "gh-pages -d dist/gh-deploy",
    "gh-b-d": "npm run gh-build && npm run gh-deploy",
    "util-ncu-install-tool": "npm i -g npm-check-updates",
    "util-ncu-install-latest": "ncu -u && npm install",
    "util-package-lock-false": "npm config set package-lock false",
    "util-fresh-install": "rm -rf node_modules && rm -rf package-lock.json && npm install"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.3",
    "@angular/animations": "6.1.8",
    "@angular/common": "6.1.8",
    "@angular/compiler": "6.1.8",
    "@angular/core": "6.1.8",
    "@angular/forms": "6.1.8",
    "@angular/http": "6.1.8",
    "@angular/platform-browser": "6.1.8",
    "@angular/platform-browser-dynamic": "6.1.8",
    "@angular/router": "6.1.8",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "g": "^2.0.1",
    "postcss": "^6.0.23",
    "postcss-merge-rules": "^2.1.2",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular-devkit/core": "0.8.3",
    "@angular/cli": "6.2.3",
    "@angular/compiler-cli": "6.1.8",
    "@angular/language-service": "6.1.8",
    "@ngtools/webpack": "^6.2.3",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.117",
    "angular-in-memory-web-api": "github:angular/in-memory-web-api",
    "autoprefixer": "^7.2.3",
    "circular-dependency-plugin": "^4.2.1",
    "codelyzer": "^4.4.4",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
